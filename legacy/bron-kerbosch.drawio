<mxfile>
  <diagram name="Bron-Kerbosch Algorithm" id="bk-flow">
    <mxGraphModel dx="1200" dy="800" grid="1" gridSize="10" guides="1" tooltips="1" connect="1" arrows="1" fold="1" page="1" pageScale="1" pageWidth="1100" pageHeight="850" math="0" shadow="1">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- compute() entry -->
        <mxCell id="2" value="compute()" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;shadow=1;arcSize=20;" vertex="1" parent="1">
          <mxGeometry x="400" y="40" width="160" height="50" as="geometry"/>
        </mxCell>

        <!-- Initialize -->
        <mxCell id="3" value="Initialize&#xa;P = all nodes&#xa;R = ∅, X = ∅" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;shadow=1;arcSize=20;" vertex="1" parent="1">
          <mxGeometry x="400" y="130" width="160" height="60" as="geometry"/>
        </mxCell>

        <!-- bk(R, P, X) -->
        <mxCell id="4" value="bk(R, P, X)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;shadow=1;arcSize=20;" vertex="1" parent="1">
          <mxGeometry x="400" y="230" width="160" height="50" as="geometry"/>
        </mxCell>

        <!-- Decision: P and X empty? -->
        <mxCell id="5" value="P empty AND&#xa;X empty?" style="rhombus;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;shadow=1;arcSize=20;" vertex="1" parent="1">
          <mxGeometry x="410" y="320" width="140" height="100" as="geometry"/>
        </mxCell>

        <!-- Maximal clique found -->
        <mxCell id="6" value="R is a maximal clique&#xa;Add to results" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;shadow=1;arcSize=20;" vertex="1" parent="1">
          <mxGeometry x="140" y="335" width="180" height="50" as="geometry"/>
        </mxCell>

        <!-- Select pivot -->
        <mxCell id="7" value="Select pivot u&#xa;(most neighbours)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;shadow=1;arcSize=20;" vertex="1" parent="1">
          <mxGeometry x="400" y="470" width="160" height="50" as="geometry"/>
        </mxCell>

        <!-- Candidates P \ N(u) -->
        <mxCell id="8" value="Candidates&#xa;P \ N(u)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;shadow=1;arcSize=20;" vertex="1" parent="1">
          <mxGeometry x="400" y="560" width="160" height="50" as="geometry"/>
        </mxCell>

        <!-- Decision: more vertices? -->
        <mxCell id="9" value="More vertices v?" style="rhombus;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;shadow=1;arcSize=20;" vertex="1" parent="1">
          <mxGeometry x="410" y="650" width="140" height="100" as="geometry"/>
        </mxCell>

        <!-- Build R', P', X' -->
        <mxCell id="10" value="R' = R ∪ {v}&#xa;P' = P ∩ N(v)&#xa;X' = X ∩ N(v)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;shadow=1;arcSize=20;" vertex="1" parent="1">
          <mxGeometry x="640" y="545" width="160" height="70" as="geometry"/>
        </mxCell>

        <!-- Recurse -->
        <mxCell id="11" value="bk(R', P', X')" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;shadow=1;arcSize=20;" vertex="1" parent="1">
          <mxGeometry x="640" y="440" width="160" height="50" as="geometry"/>
        </mxCell>

        <!-- Move v: P \ {v}, X ∪ {v} -->
        <mxCell id="12" value="P = P \ {v}&#xa;X = X ∪ {v}" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;shadow=1;arcSize=20;" vertex="1" parent="1">
          <mxGeometry x="640" y="660" width="160" height="50" as="geometry"/>
        </mxCell>

        <!-- Return -->
        <mxCell id="13" value="Return" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450;shadow=1;arcSize=20;" vertex="1" parent="1">
          <mxGeometry x="140" y="665" width="160" height="50" as="geometry"/>
        </mxCell>

        <!-- Edges -->
        <!-- compute -> initialize -->
        <mxCell id="e1" style="rounded=1;" edge="1" source="2" target="3" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- initialize -> bk -->
        <mxCell id="e2" style="rounded=1;" edge="1" source="3" target="4" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- bk -> decision -->
        <mxCell id="e3" style="rounded=1;" edge="1" source="4" target="5" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- decision -> maximal clique (Yes) -->
        <mxCell id="e4" value="Yes" style="rounded=1;" edge="1" source="5" target="6" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- decision -> select pivot (No) -->
        <mxCell id="e5" value="No" style="rounded=1;" edge="1" source="5" target="7" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- select pivot -> candidates -->
        <mxCell id="e6" style="rounded=1;" edge="1" source="7" target="8" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- candidates -> more vertices? -->
        <mxCell id="e7" style="rounded=1;" edge="1" source="8" target="9" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- more vertices? -> Return (No) -->
        <mxCell id="e8" value="No" style="rounded=1;" edge="1" source="9" target="13" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- more vertices? -> build sets (Yes) -->
        <mxCell id="e9" value="Yes" style="rounded=1;" edge="1" source="9" target="10" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- build sets -> recurse -->
        <mxCell id="e10" style="rounded=1;" edge="1" source="10" target="11" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- recurse -> move v -->
        <mxCell id="e11" style="rounded=1;" edge="1" source="11" target="12" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <!-- move v -> more vertices (loop back) -->
        <mxCell id="e12" style="rounded=1;exitX=0.5;exitY=1;exitDx=0;exitDy=0;entryX=1;entryY=0.5;entryDx=0;entryDy=0;" edge="1" source="12" target="9" parent="1">
          <mxGeometry relative="1" as="geometry">
            <Array as="points">
              <mxPoint x="870" y="710"/>
              <mxPoint x="870" y="700"/>
            </Array>
          </mxGeometry>
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
